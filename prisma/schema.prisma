generator client {
  provider         = "prisma-client-js"
  binaryTargets    = ["native", "debian-openssl-3.0.x", "rhel-openssl-3.0.x"]
  directEngineCopy = true
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  createdAt DateTime @default(now())
}